    <item android:gravity="center" android:width="50dp"
        android:top="-20dp"
        android:bottom="20dp"
        android:height="50dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle" >
                <size android:width="35.46dp"
                    android:height="35.46dp" />
                <solid android:color="#404545"/>
            </shape></rotate>
    </item>
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:height="10dp"
        android:top="40dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
        </shape>
    </item>
    <item android:top="0dp" android:bottom="25dp" >
        <shape android:shape="line">
            <stroke android:width="4dp" android:color="#000000"></stroke>
        </shape>
    </item>
